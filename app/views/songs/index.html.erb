<h1>Song Index</h1>

<ul>
<% @songs.each do |song| %>
  <li>Song: <%= link_to song.title, song_path(song) %></li>
  <li>Artist: <%= link_to song.artist_name, artist_path(song) %></li>
  <li>Edit Artist: <%= display_artist(song) %> </li><br>
<% end %>
</ul>
<br>
<%= link_to "Add New Song", new_song_path %>


